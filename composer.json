{
  "name": "mdsoroarjahan/lawlite",
  "description": "LawLite - Bilingual legal consultation platform",
  "type": "project",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0",
    "guzzlehttp/guzzle": "^7.10",
    "domnikl/statsd": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeders"
    ]
